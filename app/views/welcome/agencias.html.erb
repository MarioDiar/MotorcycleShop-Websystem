<!-- Branches page -->
<div class="container margintop-large">
  <% @branches.each do |branch|%>
  <div class="row">
    <div class="col-md-8">
      <iframe width="100%" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src=<%= @mapas[@counter].html_safe %>></iframe>
    </div>
    <div class="col-md-4">
      <h2 class="center">Sucursal <%= branch.city%></h2>
      <hr>
      <address>
        <%= branch.street%> |
        <%= branch.colony%> |
        <%= branch.postalCode%> |        
        <abbr title="Phone">Tel:</abbr><%= branch.telephone%> |
        <abbr title="Email">Email:</abbr><%= branch.email%> 
      </address>
    </div>
  </div>
  <% @counter += 1 %>
  <%end%>
</div>
<%= render "partials/footer"%>